#include<iostream>

using namespace std;
int main(){
  int a,b;
    cout<<"enter value of a :";
    cin>>a;
    cout<<"enter value of b :";
    cin>>b;
  char o;
    cout<<"enter an operator(+,-,*,/,%) :";
    cin>>o;

 switch(o){
    case '+':
    cout<<a + b;
    break;
    case '-':
    cout<<a - b;
    break;
    case '*':
    cout<<a * b;
    break;
    case '/':
    cout<<a / b;
    break;
    case '%':
    cout<<a % b;
    break;
   }
    return 0;
}
